# Intelligence Aggregator Environment Configuration
# Copy this file to .env and fill in your API keys

# VirusTotal API Key
# Get from: https://www.virustotal.com/gui/settings/api
VIRUSTOTAL_API_KEY=

# Shodan API Key  
# Get from: https://shodan.io/
SHODAN_API_KEY=

# AlienVault OTX API Key
# Get from: https://otx.alienvault.com/
OTX_API_KEY=

# IPInfo API Key (Token)
# Get from: https://ipinfo.io/
IPINFO_API_KEY=

# AbuseIPDB API Key
# Get from: https://www.abuseipdb.com/
ABUSEIPDB_API_KEY=

# URLscan API Key
# Get from: https://urlscan.io/
URLSCAN_API_KEY=

# Request timeout in seconds
REQUEST_TIMEOUT=10

# Cache settings
CACHE_ENABLED=true
CACHE_TTL=3600
